@model IEnumerable<SIERRHH.Models.Certificacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreCertificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Entidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdSector)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreCertificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Entidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSector)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCertificado">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCertificado">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCertificado">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
