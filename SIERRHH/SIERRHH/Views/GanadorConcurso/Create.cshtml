@model SIERRHH.Models.GanadorConcurso

@{
    ViewData["Title"] = "Create";
}

<style>
    body {
        background-image: url('/img/Fondo11.jpg');
        background-size: cover;
        background-repeat: no-repeat;
    }
</style>

<h5>Ganador del Concurso</h5>
<hr />

<div class="row">

