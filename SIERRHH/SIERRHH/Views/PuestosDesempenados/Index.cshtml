@model IEnumerable<SIERRHH.Models.PuestosDesempenados>

@{
    ViewData["Title"] = "Index";
}

<h1>Puestos Desempeñados</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tiempo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tiempo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPuestoDesempenado">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPuestoDesempenado">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPuestoDesempenado">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
